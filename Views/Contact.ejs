<main>

  <section id="id-Contact">
    <div class="cst-ContactContainer">
      <h1 class="cst-ContactTitle">Contact Me</h1>
    </div>

    <div class="row cst-ContactDetailsContainer">

      <div class="col-md-6 cst-DetailBlock">
          <div class="row cst-DetailRow">
            <div class="col-2 cst-DetailBox">
              <img class="cst-DetailImage" src="/Resources/Icon1.svg">
            </div>
            <div class="col-10 cst-DetailText">Bihar, India, Asia, Earth, Milky Way Galaxy, Universe</div>
          </div>
          <div class="row cst-DetailRow">
            <div class="col-2 cst-DetailBox">
              <img class="cst-DetailImage" src="/Resources/Icon2.svg">
            </div>
            <div class="col-10 cst-DetailText">+91 94628 30413</div>
          </div>
          <div class="row cst-DetailRow">
            <div class="col-2 cst-DetailBox">
              <img class="cst-DetailImage" src="/Resources/Icon3.svg">
            </div>
            <div class="col-10 cst-DetailText">alakshendra.singh@outlook.in<br>me@alakshendra.com</div>
          </div>
      </div>

      <div class="col-md-6 cst-FormContainer">
        <form id="FormContact" action="/Contact" method="POST">
          <div class="cst-FormItem"><%- ContactString %></div>
          <div class="cst-FormItem">
            <label for="ContactFormEmail" class="cst-FormLabel">Email Address<span class="cst-FormNecessary">*</span></label>
            <input id="ContactFormEmail" class="form-control FormItem" type="email" placeholder="name@example.com" name="Email" required>
          </div>
          <div class="cst-FormItem">
            <label for="ContactFormName" class="cst-FormLabel">Full Name<span class="cst-FormNecessary">*</span></label>
            <input id="ContactFormName" class="form-control FormItem" type="text" placeholder="Full Name" name="Name" required>
          </div>
          <div class="cst-FormItem">
            <label for="ContactFormMobile" class="cst-FormLabel">Mobile Number</label>
            <input id="ContactFormMobile" class="form-control FormItem" type="text" placeholder="+91 XXXXX XXXXX" name="Mobile">
          </div>
          <div class="cst-FormItem">
            <label for="ContactFormContent" class="cst-FormLabel">Questions or Suggestions<span class="cst-FormNecessary">*</span></label>
            <textarea id="ContactFormContent" class="form-control FormItem" placeholder="Questions or Suggestions" rows="3" name="Message" required></textarea>
          </div>
          <div id="FormSend" class="cst-FormItem">
            <button id="ContactFormSend" class="btn btn-outline-dark">Send</button>
          </div>
        </form>
      </div>

    </div>
  </section>

  <section id="id-Newletter">
    <h2 class="cst-NewsTitle">Subscribe To Our Newsletter</h2>
    <h4 class="cst-NewsSubTitle">Subscribe To Our Email Newsletter Updates to Stay Tuned With The News of Our Latest Endeavour.</h4>
    <form id="ProductContact" action="/Newsletter" method="POST">
      <div class="row cst-NewsFormContainer">
        <div class="col-md-6 cst-NewsInput">
          <input id="NewsletterFormEmail" class="form-control FormItem" type="email" placeholder="name@example.com" name="Email" required>
        </div>
        <div class="col-md-auto cst-NewsButton">
          <button id="SubscribeFormSend" class="btn btn-dark">Subscribe</button>
        </div>
      </div>
    </form>
  </section>

</main>
